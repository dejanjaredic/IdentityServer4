@using Microsoft.AspNetCore.Authentication
@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Claims.</p>
<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

<h2>Properties</h2>

@foreach (var prop in (await Context.AuthenticateAsync()).Properties.Items)
{
    <dt>@prop.Key</dt>
    <dt>@prop.Value</dt>
}
